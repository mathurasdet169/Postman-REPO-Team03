<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>
       
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>http</protocol>

        <!-- title of the document -->
        <documentTitle>SDET Warriors Automation</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>DS-ALGO-Extent Report</reportName>
	   
	   
	    <!-- view report without internet -->
        <offlineMode>true</offlineMode>

        <thumbnailForBase64>true</thumbnailForBase64>  <!-- for screenshot attach -->

        <!-- custom javascript -->
        <js>
        </js>

        <!-- custom styles -->
        <css></css>
    </configuration>
</extentreports>